{
  "pluginName": "汇率换算",
  "description": "😊简单易用👍提供丰富的货币转换✨可拖拽排序👀支持暗黑模式💖enjoy💖",
  "author": "HandsomeWalker",
  "main": "index.html",
  "development": {
    "main": "http://localhost:8000"
  },
  "version": "2.0.2",
  "logo": "logo.png",
  "features": [
    {
      "code": "hl",
      "explain": "汇率换算",
      "cmds": ["汇率", "货币", "money", "exchange"]
    },
    {
      "code": "fast",
      "cmds": [
        {
          "type": "regex",
          "label": "快速汇率换算",
          "match": "/^([1-9][\\d]*|0)(\\.[\\d]+)?$/"
        }
      ]
    }
  ]
}
