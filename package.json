{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build & yarn copy",
    "build:analyze": "ANALYZE=1 umi build",
    "copy": "node scripts/copyFile.js",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "mathjs": "^10.5.2",
    "react": "17.x",
    "react-dom": "17.x",
    "react-smooth-dnd": "^0.11.1",
    "react-sortable-hoc": "^2.0.0",
    "umi": "^3.5.23"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.23",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
